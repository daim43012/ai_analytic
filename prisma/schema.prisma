generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  password String  @default("defaultPassword")
  address  String?
}

model Transactions {
  id                     Int      @id @default(autoincrement())
  email                  String   @default("test@gmail.com")
  orderId                Float    @unique
  CurrencyFromChain      String   @default("")
  CurrencyFromCode       String   @default("")
  CurrencyToChain        String   @default("")
  CurrencyToCode         String   @default("")
  ClaimedIncomingAmount  Float    @default(0)
  ClaimedPublicRate      Float    @default(0)
  investDate             DateTime @default(now())
  typeOfETF              Float    @default(0)
  amountETF              Float    @default(0)
  buyPrice               Float    @default(0)
  status                 Int      @default(1)
  totalProfit            Float    @default(0)
  amount                 Float    @default(0)
  valueIncludeAmount     Float    @default(0)
  pieAmountIncludeAmount Float    @default(0)
  totalFunded            Float    @default(0)
  currencyRate           Float    @default(0)
  commission             Float    @default(0)
  commissionValue        Float    @default(0)
  amountTokens           Float    @default(0)
  isTokens               Float    @default(0)
  address                String   @default("")
  isAddedToSale          Float    @default(0)
  tokenToSale            Float    @default(0)
  isAddToTotal           Float    @default(0)
}

enum WalletType {
  REFERRAL
  BONUS
}

