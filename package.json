{
	"name": "ai_analytic",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:host": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"prod": "export PORT=4151 && node ./build/index.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"pm2": "pm2 start npm --name ai_analytic -- run prod",
		"bot": "pm2 start bot.js --name bot",
		"stripeServer": "pm2 start stripeServer.js --name stripe-server",
		"checker": "pm2 start checker.js --name checker",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prisma:gen": "prisma generate --schema=prisma/schema.prisma",
		"seed": "tsx prisma/seed.ts",
		"migrate": "prisma migrate dev --schema prisma/schema.prisma"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tanstack/svelte-query": "^5.36.0",
		"@types/bcryptjs": "^2.4.6",
		"@types/eslint": "^8.56.0",
		"@types/express": "^4.17.21",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prisma": "^5.13.0",
		"sass": "^1.77.1",
		"svelte": "^4.2.17",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"tsx": "^4.8.2",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@amcharts/amcharts5": "^5.9.8",
		"@ethersproject/contracts": "^5.7.0",
		"@ethersproject/providers": "^5.7.2",
		"@prisma/client": "^5.13.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@uniswap/widgets": "^2.59.0",
		"@walletconnect/web3-provider": "^1.8.0",
		"apexcharts": "^3.49.1",
		"axios": "^1.6.8",
		"bcrypt": "^5.1.1",
		"bcryptjs": "^2.4.3",
		"body-parser": "^2.2.0",
		"chart.js": "^4.4.2",
		"cors": "^2.8.5",
		"d3": "^7.9.0",
		"decimal.js": "^10.4.3",
		"dotenv": "^16.4.5",
		"echarts": "^5.5.0",
		"ethers": "^5.7.2",
		"express": "^4.21.2",
		"jsonwebtoken": "^9.0.2",
		"lightweight-charts": "^5.0.6",
		"marked": "^15.0.7",
		"node-fetch": "^3.3.2",
		"node-telegram-bot-api": "^0.66.0",
		"openai": "^4.47.1",
		"prisma-client": "^0.0.0",
		"react-dom": "^18.3.1",
		"redis": "^4.6.13",
		"sqlite3": "^5.1.7",
		"stripe": "^18.0.0",
		"svelte-apexcharts": "^1.0.2",
		"svelte-chartjs": "^3.1.5",
		"svelte-icons-pack": "^3.1.3",
		"svelte-preprocess-react": "^0.17.0",
		"web3": "^4.8.0",
		"web3modal": "^1.9.12"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	}
}
